{
    "name": "adev/lrc",
    "description": "A package to generate column constants for Laravel models",
    "license": "MIT",
    "homepage": "https://adev.ma/",
    "type": "library",
    "keywords": [
        "columns",
        "constants"
    ],
    "authors": [
        {
            "name": "adil elkhalloufi",
            "email": "adilelkhalloufi@gmail.com"
        }
    ],
  
    "require": {
        "php": ">=7.4",
        
         
     },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
             "Lrc\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lrc\\LRCServiceProvider"
            ]
        }
    }
 
}
